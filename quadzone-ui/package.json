        "axios": "^1.13.2",
        "chart.js": "^4.5.1",
        "ckeditor5": "^47.1.0",
        "date-fns": "^4.1.0",
        "dayjs": "^1.11.19",
        "es-toolkit": "^1.34.1",
        "formik": "^2.4.9",
